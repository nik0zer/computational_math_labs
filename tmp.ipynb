    "\n",
    "\n",
    "import numpy as np\n",
    "\n",
    "def f(x):\n",
    "    return np.log(1 + x ** 2 / 3)\n",
    "\n",
    "x = np.linspace(-1, 1, 13)\n",
    "\n",
    "sum = f(x[0])\n",
    "for i in range(1, len(x) - 1):\n",
    "    sum += 2 * f(x[i])\n",
    "\n",
    "sum += f(x[len(x) - 1])\n",
    "\n",
    "sum *= 2 / (2 * 12)\n",
    "sum"
   ]
  }
